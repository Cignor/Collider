{
  "version": 1,
  "engine": {
    "sample_rate": 48000,
    "buffer_size": 512,
    "mix_queue_target": 6
  },
  "dsp": {
    "signalsmith": {
      "pitch_shift_semitones": 0.0,
      "tempo_factor": 1.0,
      "mix": 1.0
    }
  },
  "effects": {
    "active_preset": "default",
    "presets_dir": "audio/audio_effects/presets"
  },
  "spatial": {
    "pan_smoothing_ms": 30,
    "spatial_near_ratio": 0.12
  },
  "generators": {
    "frequencies": {
      "attack_ms": 1,
      "release_ms": 50,
      "waveform": "sine"
    },
    "noise": {
      "color": "white",
      "cutoff_hz": 8000
    }
  }
}


